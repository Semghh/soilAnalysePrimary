<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.content2.Mapper.Secondary.RecordMapper">
    <insert id="insertNewResultRecord">
        insert into `result_record`
        (`database_date`,`calculate_date`,`offset`,`magnification`,`average_append_times`,
         `average_response`,`loop_times`,`average_region_size`)
         values (#{database_date},#{calculate_date},#{offset},#{magnification},#{average_append_times},
                 #{average_response},#{loop_times},#{average_region_size})
    </insert>
</mapper>