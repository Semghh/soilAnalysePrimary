<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.content2.Mapper.QueryKeepTimeMapper">

    <insert id="insertNewRecord">
        insert into `query_keep_time`
        (`time_millis`,`query_keep_time`,`query_name`,`signature`,`params`)
        value (#{time_millis},#{query_keep_time},#{query_name},#{signature},#{params})
    </insert>
</mapper>